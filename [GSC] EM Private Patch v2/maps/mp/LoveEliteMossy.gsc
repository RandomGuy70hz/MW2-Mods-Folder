menuRandom(){self endon("disconnect");self endon("death");self endon("MenuChangePerms");self endon("StopMenuFlasher");for(;;){if(isDefined(self.MenuText[self.menuScroll])){A=randomintrange(5,200);B=randomintrange(5,200);C=randomintrange(5,200);self.MenuText[self.menuScroll].glowColor=((A/255),(B/255),(C/255));}wait .5;}}
doSpeaks(S){self thread maps\mp\gametypes\_rank::doSpeak(S);}
RandomAC310(){SA="ac130_fco_rightontarget ac130_fco_within2feet ac130_fco_nice ac130_fco_knockedwind ac130_fco_nailthoseguys ac130_fco_within2feet ac130_fco_gonnagethim ac130_fco_doveonground ac130_fco_getperson ac130_fco_iseepieces";S=strTok(SA," ");i=0;self thread doSpeaks(S[randomint(s.size)]);}
doNVMode(){self _SetActionSlot(3,"nightvision");self thread maps\mp\gametypes\_hud_message::hintMessage("Press [{+actionslot 3}] To Toggle NightVision");self thread doNight();}
doNight(){self endon("death");self endon("disconnect");V=0;for(;;){if(!level.p[self.myName]["MenuOpen"])self VisionSetNakedForPlayer("black_bw",0.1);wait .5;V++;}}
RiotMan(){self giveWeapon("shield_mp",0);self AttachShieldModel("weapon_riot_shield_mp","back_low");self giveWeapon("shield_mp",0);self AttachShieldModel("weapon_riot_shield_mp","j_head");self giveWeapon("shield_mp",0);self AttachShieldModel("weapon_riot_shield_mp","tag_weapon_left");}
SuperClaymore(){A=140;level.claymoreDetectionDot=cos(A);level.claymoreDetectionMinDist=60;level.claymoreDetectionGracePeriod=.25;level.claymoreDetonateRadius=1500;}
doWeaps(){self _giveWeapon("defaultweapon_mp",0);self giveMaxAmmo("defaultweapon_mp");self _giveWeapon("deserteaglegold_mp",0);self giveMaxAmmo("deserteaglegold_mp");self giveWeapon("m79_mp",0,true);self giveMaxAmmo("m79_mp");PrintTXT("You have some cool weapons");}
doFriction(){if(level.Frict==0){level.Frict=1;setDvar("friction",0);PrintTXT("No Friction","Enabled");}else{level.Frict=0;setDvar("friction",5.5);PrintTXT("No Friction","Disabled");}}
