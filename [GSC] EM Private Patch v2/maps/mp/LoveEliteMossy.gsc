CEV(enter,exit,angle){flag=spawn("script_model",enter);FlagFX=loadfx("misc/flare_ambient");playfx(FlagFX,flag.origin);flag setModel(level.elevator_model["enter"]);wait 0.1;flag=spawn("script_model",exit);flag setModel(level.elevator_model["exit"]);playfx(FlagFX,flag.origin);wait .01;self thread ETH(enter,exit,angle);}
ETH(enter,exit,angle){self endon("disconnect");while(1){foreach(player in level.players){if(Distance(enter,player.origin) <= 50){player SetOrigin(exit);player SetPlayerAngles(angle);}}wait .25;}}
