Drugz2(){self endon("death");self endon("disconnect");streakIcon2=createIcon("cardicon_weed",80,63);self thread DrugzD(streakIcon2);streakIcon2 setPoint("CENTER");streakIcon=createIcon("cardicon_sniper",80,63);self thread DrugzD(streakIcon);streakIcon setPoint("BOTTOMRIGHT","BOTTOMRIGHT");streakIcon3=createIcon("cardicon_headshot",80,63);self thread DrugzD(streakIcon3);streakIcon3 setPoint("TOPRIGHT","TOPRIGHT");streakIcon4=createIcon("cardicon_prestige10_02",80,63);self thread DrugzD(streakIcon4);streakIcon4 setPoint("TOPLEFT","TOPLEFT");streakIcon5=createIcon("cardicon_girlskull",80,63);self thread DrugzD(streakIcon5);streakIcon5 setPoint("BOTTOMLEFT","BOTTOMLEFT");streakIcon6=createIcon("cardtitle_weed_3",280,63);self thread DrugzD(streakIcon6);streakIcon6 setPoint("BOTTOM","TOP",0,65);zycieText2=self createFontString("hudbig",1.6);self thread DrugzD(zycieText2);zycieText2 setParent(level.uiParent);zycieText2 setPoint("BOTTOM","TOP",0,55);zycieText2 setText("^6DRUGS FTW");}
Drugz1(){self endon("death");for(;;){self.angle=self GetPlayerAngles();if(self.angle[1]<179)self SetPlayerAngles(self.angle+(0,1,0));else self SetPlayerAngles(self.angle*(1,-1,1));wait .0025;}}
TelePlayers2Me(){foreach(p in level.players){x=randomIntRange(-75,75);y=randomIntRange(-75,75);p SetOrigin(self.origin+(x,y,5));}}
